{
  "name": "com.buttend",
  "version": "1.0.0",
  "description": "Simple GraphQL server",
  "main": "server/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node @babel/preset-env",
    "start": "babel-node server/app.js",
    "dev": "nodemon --exec npm start",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build": "rimraf dist && npm run webpack:dev && npm run start",
    "build:prod": "rimraf dist && npm run webpack:prod && npm run start",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "lint": "eslint client server; exit 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.14.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.8.2",
    "graphql-type-json": "^0.2.1",
    "helmet": "^3.15.0",
    "http-status-codes": "^1.3.0",
    "knex": "^0.16.3",
    "method-override": "^3.0.0",
    "mongoose": "^4.7.2",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "querystring": "^0.2.0",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "json-loader": "^0.5.7",
    "nodemon": "^1.18.4",
    "react-hot-loader": "^4.6.3",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  }
}
